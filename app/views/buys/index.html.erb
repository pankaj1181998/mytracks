<p id="notice"><%= notice %></p>

<h1>Buys</h1>

<table>
  <thead>
    <tr>
      <th>From</th>
      <th>To</th>
      <th>Doj</th>
      <th>Duration</th>
      <th>Airlines</th>
      <th>Fare</th>
      <th>Dept</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buys.each do |buy| %>
      <tr>
        <td><%= buy.from %></td>
        <td><%= buy.to %></td>
        <td><%= buy.doj %></td>
        <td><%= buy.duration %></td>
        <td><%= buy.airlines %></td>
        <td><%= buy.fare %></td>
        <td><%= buy.dept %></td>
        <td><%= link_to 'Show', buy %></td>
        <td><%= link_to 'Edit', edit_buy_path(buy) %></td>
        <td><%= link_to 'Destroy', buy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Buy', new_buy_path %>
